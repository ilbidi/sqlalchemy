# Modelli per gestione tabelle
import sqlalchemy
import json

from sqlalchemy import Columnm, Integer, String

class User(Base):
    # Table name
    __tablename__ = 'user'
    # Fields
    id = Column(Integer, primary_key = True)
    name = Column(String)
    fullname = Column(String)
    password = Column(String)

    # Show content
    def __repr__(self):
        return json.dumps(dict(self))
